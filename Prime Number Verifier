# Create a program that will take a number as an input and output "True" if it is a prime number, or "False" if it is not
def prime(num):
    if num == 2:
        return True
    if num == 1:
        return False
    for i in range(2,num):
        if num % i == 0:
            return False
    return True

print(prime(17))
